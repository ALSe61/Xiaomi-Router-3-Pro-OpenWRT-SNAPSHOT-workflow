[ "$(uci -q get wireless.@wifi-device[0].disabled)" != "0" ] && exit 0
uci -q batch << EOI
set wireless.@wifi-device[0].disabled='0'
set wireless.@wifi-iface[0].ssid='OpenWrt'
set wireless.@wifi-iface[0].encryption='psk-mixed'
set wireless.@wifi-iface[0].key='0123456789'
set wireless.@wifi-iface[0].country='RU'
EOI
uci commit
service network restart
exit 0
